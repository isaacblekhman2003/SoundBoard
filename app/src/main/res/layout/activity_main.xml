<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SoundboardActivity">

    <Button
        android:id="@+id/button_main_Bb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="B♭"
        app:layout_constraintBottom_toTopOf="@+id/button_main_B"
        app:layout_constraintEnd_toStartOf="@+id/button_main_song"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_play"
        app:layout_constraintTop_toBottomOf="@+id/button_main_A" />

    <Button
        android:id="@+id/button_main_B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="B"
        app:layout_constraintBottom_toTopOf="@+id/button_main_C"
        app:layout_constraintEnd_toStartOf="@+id/button_main_song"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_play"
        app:layout_constraintTop_toBottomOf="@+id/button_main_Bb" />

    <Button
        android:id="@+id/button_main_C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C"
        app:layout_constraintBottom_toTopOf="@+id/button_main_CC"
        app:layout_constraintEnd_toStartOf="@+id/button_main_song"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_play"
        app:layout_constraintTop_toBottomOf="@+id/button_main_B" />

    <Button
        android:id="@+id/button_main_CC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C♯"
        app:layout_constraintBottom_toTopOf="@+id/button_main_D"
        app:layout_constraintEnd_toStartOf="@+id/button_main_song"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_play"
        app:layout_constraintTop_toBottomOf="@+id/button_main_C" />

    <Button
        android:id="@+id/button_main_D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D"
        app:layout_constraintBottom_toTopOf="@+id/button_main_DD"
        app:layout_constraintEnd_toStartOf="@+id/button_main_song"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_play"
        app:layout_constraintTop_toBottomOf="@+id/button_main_CC" />

    <Button
        android:id="@+id/button_main_DD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D♯"
        app:layout_constraintBottom_toTopOf="@+id/button_main_E"
        app:layout_constraintEnd_toStartOf="@+id/button_main_song"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_play"
        app:layout_constraintTop_toBottomOf="@+id/button_main_D" />

    <Button
        android:id="@+id/button_main_E"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E"
        app:layout_constraintBottom_toTopOf="@+id/button_main_clear"
        app:layout_constraintEnd_toStartOf="@+id/button_main_clear"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_clear"
        app:layout_constraintTop_toBottomOf="@+id/button_main_DD" />

    <Button
        android:id="@+id/button_main_A"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A"
        app:layout_constraintBottom_toTopOf="@+id/button_main_Bb"
        app:layout_constraintEnd_toStartOf="@+id/button_main_song"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_play"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_main_song"
        android:layout_width="115dp"
        android:layout_height="733dp"
        android:text="Song"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_clear"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_main_play"
        android:layout_width="142dp"
        android:layout_height="733dp"
        android:text="PlayBack"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_main_clear"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_main_clear"
        android:layout_width="146dp"
        android:layout_height="45dp"
        android:text="Clear"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_main_song"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_main_play"
        app:layout_constraintTop_toBottomOf="@+id/button_main_E" />
</androidx.constraintlayout.widget.ConstraintLayout>